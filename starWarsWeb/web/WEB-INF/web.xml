<?xml version="1.0" encoding="UTF-8"?>
<web-app version="2.4" xmlns="http://java.sun.com/xml/ns/j2ee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/j2ee          http://java.sun.com/xml/ns/j2ee/web-app_2_4.xsd">
    <display-name>starWarsWeb</display-name>
    <servlet>
        <servlet-name>FilmSuppression</servlet-name>
        <servlet-class>Controllers.FilmSuppression</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>FilmListe</servlet-name>
        <servlet-class>Controllers.FilmListe</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>FilmSaisi</servlet-name>
        <servlet-class>Controllers.FilmSaisi</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>FilmSaisiResultat</servlet-name>
        <servlet-class>Controllers.FilmSaisiResultat</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>FilmMiseAJour</servlet-name>
        <servlet-class>Controllers.FilmMiseAJour</servlet-class>  
    </servlet>
    <servlet>
        <servlet-name>AvisSaisi</servlet-name>
        <servlet-class>Controllers.AvisSaisi</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AvisSaisiResultat</servlet-name>
        <servlet-class>Controllers.AvisSaisiResultat</servlet-class>
    </servlet>
    
    <servlet-mapping>
        <servlet-name>FilmSaisi</servlet-name>
        <url-pattern>/FilmSaisi</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>FilmSaisiResultat</servlet-name>
        <url-pattern>/FilmSaisiResultat</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>FilmMiseAJour</servlet-name>
        <url-pattern>/FilmMiseAJour</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>FilmListe</servlet-name>
        <url-pattern>/FilmListe</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>FilmSuppression</servlet-name>
        <url-pattern>/FilmSuppression</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AvisSaisi</servlet-name>
        <url-pattern>/AvisSaisi</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AvisSaisiResultat</servlet-name>
        <url-pattern>/AvisSaisiResultat</url-pattern>
    </servlet-mapping>
</web-app>
